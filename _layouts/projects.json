{% assign titles = site.projects | map: "title" |  %}

{
  "projects": [
    {% for project in site.projects %}
    {
      "id": "{{ project.id }}",
      "title": "{{ project.title }}",
      "thumbnail": "{{ project.thumbnail }}",
      "images": [
        {% for image in project.images %}
        "{{ image }}"{% if image != project.images.last %},{% endif %}
        {% endfor %}
      ]
    }{% if project != site.projects.last %},{% endif %}
    {% endfor %}
  ]
}
